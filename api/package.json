{
  "name": "server",
  "module": "server.ts",
  "type": "module",
  "engines": {
    "node": "20.16.0"
  },
  "scripts": {
    "build": "tsup src --out-dit build",
    "start": "node build/server.js",
    "start:dev": "tsx watch src/server.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@prisma/client": "5.18.0",
    "fastify": "4.26.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/node": "20.14.15",
    "@vitest/coverage-v8": "2.0.5",
    "dotenv": "16.4.5",
    "prisma": "5.19.1",
    "tsup": "8.2.4",
    "tsx": "4.17.0",
    "typescript": "5.5.4",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.0.5"
  }
}
